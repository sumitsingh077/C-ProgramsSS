#include <stdio.h>

void convertTime(int seconds);

int main() {
    int sec;
    printf("Enter seconds: ");
    scanf("%d", &sec);
    convertTime(sec);
    return 0;
}

void convertTime(int seconds) {
    int hrs = seconds / 3600;
    int min = (seconds % 3600) / 60;
    int sec = seconds % 60;

    printf("Time = %d:%d:%d\n", hrs, min, sec);
}
