#include <stdio.h>

char findGrade(int marks);

int main() {
    int marks;
    printf("Enter marks: ");
    scanf("%d", &marks);

    printf("Grade = %c\n", findGrade(marks));
    return 0;
}

char findGrade(int marks) {
    if (marks >= 90) return 'A';
    else if (marks >= 80) return 'B';
    else if (marks >= 70) return 'C';
    else if (marks >= 60) return 'D';
    else return 'F';
}
