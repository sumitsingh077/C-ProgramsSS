#include <stdio.h>

int sumDigits(int n);

int main() {
    int num;
    printf("Enter a five-digit number: ");
    scanf("%d", &num);

    printf("Sum of digits = %d\n", sumDigits(num));
    return 0;
}

int sumDigits(int n) {
    int sum = 0;
    while (n > 0) {
        sum += n % 10;
        n /= 10;
    }
    return sum;
}
